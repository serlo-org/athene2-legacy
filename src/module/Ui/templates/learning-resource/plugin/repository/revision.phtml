<?php if(is_object($this->revision)): ?>
<article class="revision">
	<?php foreach($this->plugin->getFields() as $field): ?>
	<section><?php echo $this->revision->get($field); ?></section>
	<?php endforeach; ?>
</article>
<?php else: ?>
<article class="revision">
    <?php echo $this->translate('Es wurde noch keine Revision gesetzt.'); ?>
</article>
<?php endif; ?>